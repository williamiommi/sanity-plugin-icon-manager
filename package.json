{
  "name": "sanity-plugin-icon-manager",
  "version": "1.0.0",
  "description": "A Sanity plugin for selecting, managing, and customizing icons.",
  "keywords": [
    "sanity",
    "sanity-plugin",
    "sanity-plugin-icon-manager"
  ],
  "homepage": "https://github.com/williamiommi/sanity-plugin-icon-manager#readme",
  "bugs": {
    "url": "https://github.com/williamiommi/sanity-plugin-icon-manager/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/williamiommi/sanity-plugin-icon-manager.git"
  },
  "license": "MIT",
  "author": "William Iommi <william.iommi@gmail.com>",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "source": "./src/index.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.esm.js",
      "default": "./dist/index.esm.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "run-s clean && plugin-kit verify-package --silent && pkg-utils build --strict && pkg-utils --strict",
    "clean": "rimraf dist",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepublishOnly": "run-s build",
    "watch": "pkg-utils watch --strict",
    "prepare": "husky install"
  },
  "dependencies": {
    "@iconify/react": "^4.1.1",
    "@iconify/utils": "^2.1.9",
    "@sanity/incompatible-plugin": "^1.0.4",
    "@sanity/ui": "^1.7.9",
    "dompurify": "^3.0.5",
    "react-colorful": "^5.6.1",
    "zustand": "^4.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@iconify/types": "^2.0.0",
    "@sanity/pkg-utils": "^2.3.10",
    "@sanity/plugin-kit": "^3.1.7",
    "@sanity/semantic-release-preset": "^4.1.2",
    "@types/dompurify": "^3.0.2",
    "@types/react": "^18.2.18",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-config-sanity": "^6.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.1",
    "prettier-plugin-packagejson": "^2.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rimraf": "^5.0.1",
    "sanity": "^3.15.1",
    "styled-components": "^5.2",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "react": "^18",
    "sanity": "^3"
  },
  "engines": {
    "node": ">=14"
  }
}
